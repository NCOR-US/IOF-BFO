<http://www.industrialontologies.org/core> rdf:type owl:Ontology ;
                                            owl:imports bfoo:bfo.owl ;
                                            rdfs:comment "This ontology is intended to reflect the Top-Down group, a working group of the Industrial Ontology Foundry (IOF). It is an OWL implementation of the ontology described in https://docs.google.com/document/d/1gwrGTOjFc3xdnfUsMVNGOHwasUQUBpKXxwyrKlC5UBI/edit" ;
                                            owl:versionInfo ".001" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.industrialontologies.org/core#definition-FOL
iof:definition-FOL rdf:type owl:AnnotationProperty ;
                   rdfs:comment "An m expressed in first order logic using CLIF syntax"@en ;
                   rdfs:label "definition-FOL"@en .


###  http://www.industrialontologies.org/core#definition-NL
iof:definition-NL rdf:type owl:AnnotationProperty ;
                  rdfs:comment "An axiom associated with a term expressed using natural language"@en ;
                  rdfs:label "definition-NL"@en .


###  http://www.industrialontologies.org/core#definition-SemiFOL
iof:definition-SemiFOL rdf:type owl:AnnotationProperty ;
                       rdfs:label "definition-SemiFOL" .


###  http://www.industrialontologies.org/core#definition_other_common
iof:definition_other_common rdf:type owl:AnnotationProperty ;
                            rdfs:label "x-definitions - other common" .


###  http://www.industrialontologies.org/core#example_complement
iof:example_complement rdf:type owl:AnnotationProperty ;
                       rdfs:label "x-examples (complement)" ;
                       rdfs:subPropertyOf cco:example_of_usage .


###  http://www.industrialontologies.org/core#example_counter
iof:example_counter rdf:type owl:AnnotationProperty ;
                    rdfs:label "x-examples (x-counter)" ;
                    rdfs:subPropertyOf cco:example_of_usage .


###  http://www.industrialontologies.org/core#source
iof:source rdf:type owl:AnnotationProperty ;
           cco:definition "A citation of where all or some of the information used to create the term's Definition was acquired from." ;
           rdfs:label "source"@en .


###  http://www.industrialontologies.org/core#unresolved_issues
iof:unresolved_issues rdf:type owl:AnnotationProperty ;
                      rdfs:label "x-unresolved issues" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/definition
cco:definition rdf:type owl:AnnotationProperty ;
               cco:definition "A natural language explication of the meaning of the term." ;
               cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology" ;
               rdfs:label "definition"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/elucidation
cco:elucidation rdf:type owl:AnnotationProperty ;
                cco:definition "A clarification or further explanation of a term beyond what is included in the Definition or which is used when the term is primitive such that no non-circular definition can be given for it." ;
                cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology" ;
                rdfs:label "elucidation"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/example_of_usage
cco:example_of_usage rdf:type owl:AnnotationProperty ;
                     rdfs:label "example of usage"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/is_curated_in_ontology
cco:is_curated_in_ontology rdf:type owl:AnnotationProperty ;
                           cco:definition "An annotation property that links a class, property, or named individual to the URI of the ontology where it is located." ;
                           cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology" ;
                           rdfs:label "is curated in ontology"@en .


#################################################################
#    Object Properties
#################################################################

###  http://purl.obolibrary.org/obo/BFO_0000051
bfoo:BFO_0000051 rdf:type owl:ObjectProperty .


###  http://www.industrialontologies.org/core#RO_0000079
iof:RO_0000079 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bfoo:BFO_0000197 ;
               owl:inverseOf iof:RO_0000085 ;
               rdfs:domain bfoo:BFO_0000034 ;
               cco:definition "A function inheres in its bearer at all times for which the function exists, however the function need not be realized at all the times that the function exists."@en ,
                              "a relation between a function and an independent continuant (the bearer), in which the function specifically depends on the bearer for its existence"@en ;
               cco:example_of_usage "this catalysis function is a function of this enzyme"@en ;
               rdfs:label "function of"@en ;
               skos:altLabel "function_of"@en ,
                             "is function of"@en .


###  http://www.industrialontologies.org/core#RO_0000080
iof:RO_0000080 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bfoo:BFO_0000197 ;
               owl:inverseOf iof:RO_0000086 ;
               cco:definition "A quality inheres in its bearer at all times for which the quality exists."@en ,
                              "a relation between a quality and an independent continuant (the bearer), in which the quality specifically depends on the bearer for its existence"@en ;
               cco:example_of_usage "this red color is a quality of this apple"@en ;
               rdfs:label "quality of"@en ;
               skos:altLabel "is quality of"@en ,
                             "quality_of"@en .


###  http://www.industrialontologies.org/core#RO_0000081
iof:RO_0000081 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bfoo:BFO_0000197 ;
               owl:inverseOf iof:RO_0000087 ;
               cco:definition "A role inheres in its bearer at all times for which the role exists, however the role need not be realized at all the times that the role exists."@en ,
                              "a relation between a role and an independent continuant (the bearer), in which the role specifically depends on the bearer for its existence"@en ;
               cco:example_of_usage "this investigator role is a role of this person"@en ;
               rdfs:label "role of"@en ;
               skos:altLabel "is role of"@en ,
                             "role_of"@en .


###  http://www.industrialontologies.org/core#RO_0000085
iof:RO_0000085 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bfoo:BFO_0000196 ;
               rdfs:domain bfoo:BFO_0000004 ;
               rdfs:range bfoo:BFO_0000034 ;
               cco:definition "A bearer can have many functions, and its functions can exist for different periods of time, but none of its functions can exist when the bearer does not exist. A function need not be realized at all the times that the function exists."@en ,
                              "a relation between an independent continuant (the bearer) and a function, in which the function specifically depends on the bearer for its existence"@en ;
               cco:example_of_usage "this enzyme has function this catalysis function (more colloquially: this enzyme has this catalysis function)"@en ;
               rdfs:label "has function"@en .


###  http://www.industrialontologies.org/core#RO_0000086
iof:RO_0000086 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bfoo:BFO_0000196 ;
               rdfs:range bfoo:BFO_0000019 ;
               cco:definition "A bearer can have many qualities, and its qualities can exist for different periods of time, but none of its qualities can exist when the bearer does not exist."@en ,
                              "a relation between an independent continuant (the bearer) and a quality, in which the quality specifically depends on the bearer for its existence"@en ;
               cco:example_of_usage "this apple has quality this red color"@en ;
               rdfs:label "has quality"@en .


###  http://www.industrialontologies.org/core#RO_0000087
iof:RO_0000087 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bfoo:BFO_0000196 ;
               rdfs:domain bfoo:BFO_0000004 ;
               rdfs:range bfoo:BFO_0000023 ;
               cco:definition "A bearer can have many roles, and its roles can exist for different periods of time, but none of its roles can exist when the bearer does not exist. A role need not be realized at all the times that the role exists."@en ,
                              "a relation between an independent continuant (the bearer) and a role, in which the role specifically depends on the bearer for its existence"@en ;
               cco:example_of_usage "this person has role this investigator role (more colloquially: this person has this role of investigator)"@en ;
               rdfs:label "has role"@en .


###  http://www.industrialontologies.org/core#RO_0000091
iof:RO_0000091 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bfoo:BFO_0000196 ;
               owl:inverseOf iof:RO_0000092 ;
               rdfs:domain bfoo:BFO_0000004 ;
               rdfs:range bfoo:BFO_0000016 ;
               cco:definition "a relation between an independent continuant (the bearer) and a disposition, in which the disposition specifically depends on the bearer for its existence"@en ;
               rdfs:label "has disposition"@en .


###  http://www.industrialontologies.org/core#RO_0000092
iof:RO_0000092 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bfoo:BFO_0000197 ;
               cco:definition "inverse of has disposition" ;
               rdfs:label "disposition of"@en .


###  http://www.industrialontologies.org/core#RO_0001000
iof:RO_0001000 rdf:type owl:ObjectProperty ;
               owl:inverseOf iof:RO_0001001 ;
               cco:definition "This is a very general relation. More specific relations are preferred when applicable, such as 'directly develops from'."@en ,
                              "a relation between two distinct material entities, the new entity and the old entity, in which the new entity begins to exist when the old entity ceases to exist, and the new entity inherits the significant portion of the matter of the old entity"@en ;
               cco:example_of_usage "this cell derives from this parent cell (cell division)"@en ,
                                    "this nucleus derives from this parent nucleus (nuclear division)"@en ;
               rdfs:label "derives from"@en .


###  http://www.industrialontologies.org/core#RO_0001001
iof:RO_0001001 rdf:type owl:ObjectProperty ;
               cco:definition "This is a very general relation. More specific relations are preferred when applicable, such as 'directly develops into'. To avoid making statements about a future that may not come to pass, it is often better to use the backward-looking 'derives from' rather than the forward-looking 'derives into'."@en ,
                              "a relation between two distinct material entities, the old entity and the new entity, in which the new entity begins to exist when the old entity ceases to exist, and the new entity inherits the significant portion of the matter of the old entity"@en ;
               cco:example_of_usage "this parent cell derives into this cell (cell division)"@en ,
                                    "this parent nucleus derives into this nucleus (nuclear division)"@en ;
               rdfs:label "derives into"@en .


###  http://www.industrialontologies.org/core#RO_0001015
iof:RO_0001015 rdf:type owl:ObjectProperty ;
               owl:inverseOf iof:RO_0001025 ;
               rdf:type owl:TransitiveProperty ;
               cco:definition "Most location relations will only hold at certain times, but this is difficult to specify in OWL. See https://code.google.com/p/obo-relations/wiki/ROAndTime"@en ,
                              "a relation between two independent continuants, the location and the target, in which the target is entirely within the location"@en ;
               cco:example_of_usage "my head is the location of my brain"@en ,
                                    "this cage is the location of this rat"@en ;
               rdfs:label "location of"@en .


###  http://www.industrialontologies.org/core#RO_0001018
iof:RO_0001018 rdf:type owl:ObjectProperty ;
               owl:inverseOf iof:RO_0001019 ;
               rdfs:domain bfoo:BFO_0000040 ;
               rdfs:range bfoo:BFO_0000004 ;
               owl:propertyChainAxiom ( iof:RO_0001025
                                        bfoo:BFO_0000171
                                      ) ;
               cco:definition "Containment is location not involving parthood, and arises only where some immaterial continuant is involved." ,
                              "Containment obtains in each case between material and immaterial continuants, for instance: lung contained_in thoracic cavity; bladder contained_in pelvic cavity. Hence containment is not a transitive relation.    If c part_of c1 at t then we have also, by our definition and by the axioms of mereology applied to spatial regions, c located_in c1 at t. Thus, many examples of instance-level location relations for continuants are in fact cases of instance-level parthood. For material continuants location and parthood coincide. Containment is location not involving parthood, and arises only where some immaterial continuant is involved. To understand this relation, we first define overlap for continuants as follows:    c1 overlap c2 at t =def for some c, c part_of c1 at t and c part_of c2 at t. The containment relation on the instance level can then be defined (see definition):"@en ,
                              """Intended meaning:
domain: material entity
range: spatial region or site (immaterial continuant)
        """@en ;
               rdfs:label "contained in"@en .


###  http://www.industrialontologies.org/core#RO_0001019
iof:RO_0001019 rdf:type owl:ObjectProperty ;
               rdfs:label "contains"@en .


###  http://www.industrialontologies.org/core#RO_0001025
iof:RO_0001025 rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain bfoo:BFO_0000004 ;
               rdfs:range bfoo:BFO_0000004 ;
               iof:source "http://www.obofoundry.org/ro/#OBO_REL:located_in" ;
               cco:definition "Location as a relation between instances: The primitive instance-level relation c located_in r at t reflects the fact that each continuant is at any given time associated with exactly one spatial region, namely its exact location. Following we can use this relation to define a further instance-level location relation - not between a continuant and the region which it exactly occupies, but rather between one continuant and another. c is located in c1, in this sense, whenever the spatial region occupied by c is part_of the spatial region occupied by c1.    Note that this relation comprehends both the relation of exact location between one continuant and another which obtains when r and r1 are identical (for example, when a portion of fluid exactly fills a cavity), as well as those sorts of inexact location relations which obtain, for example, between brain and head or between ovum and uterus"@en ,
                              "Most location relations will only hold at certain times, but this is difficult to specify in OWL. See https://code.google.com/p/obo-relations/wiki/ROAndTime"@en ,
                              "a relation between two independent continuants, the target and the location, in which the target is entirely within the location"@en ;
               cco:example_of_usage "my brain is located in my head"@en ,
                                    "this rat is located in this cage"@en ;
               rdfs:label "located in"@en .


###  http://www.industrialontologies.org/core#RO_0002000
iof:RO_0002000 rdf:type owl:ObjectProperty ;
               owl:inverseOf iof:RO_0002002 ;
               cco:definition "A 2D boundary may have holes and gaps, but it must be a single connected entity, not an aggregate of several disconnected parts."@en ,
                              "Although the boundary is two-dimensional, it exists in three-dimensional space and thus has a 3D shape."@en ,
                              "a relation between a 2D immaterial entity (the boundary) and a material entity, in which the boundary delimits the material entity"@en ;
               cco:example_of_usage "the surface of my skin is a 2D boundary of my body"@en ;
               rdfs:label "2D boundary of"@en ;
               skos:altLabel "2D_boundary_of"@en ,
                             "boundary of"@en ,
                             "is 2D boundary of"@en ,
                             "is boundary of"@en .


###  http://www.industrialontologies.org/core#RO_0002002
iof:RO_0002002 rdf:type owl:ObjectProperty ;
               rdfs:domain bfoo:BFO_0000040 ;
               rdfs:range bfoo:BFO_0000141 ;
               cco:definition "A 2D boundary may have holes and gaps, but it must be a single connected entity, not an aggregate of several disconnected parts."@en ,
                              "Although the boundary is two-dimensional, it exists in three-dimensional space and thus has a 3D shape."@en ,
                              "a relation between a material entity and a 2D immaterial entity (the boundary), in which the boundary delimits the material entity"@en ;
               cco:example_of_usage "my body has 2D boundary the surface of my skin"@en ;
               rdfs:label "has 2D boundary"@en ;
               skos:altLabel "has boundary"@en ,
                             "has_2D_boundary"@en .


###  http://www.industrialontologies.org/core#RO_0010002
iof:RO_0010002 rdf:type owl:ObjectProperty ;
               rdfs:domain bfoo:BFO_0000004 ;
               rdfs:range bfoo:BFO_0000031 ;
               owl:propertyChainAxiom ( bfoo:BFO_0000059
                                        bfoo:BFO_0000196
                                      ) ;
               iof:source "[072-ISO]" ;
               cco:definition "*b* is carrier of *c* at time *t* if and only if *c* *g-depends on* *b* at *t*" ;
               cco:example_of_usage "Molecules of DNA are carriers of genetic information." ,
                                    "This copy of *War and Peace* is carrier of the novel written by Tolstoy." ,
                                    "This hard drive is carrier of these data items." ;
               rdfs:label "is carrier of"@en .


###  http://www.industrialontologies.org/core#achieves
iof:achieves rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             iof:definition-FOL "achieves(p,o) ‚Üí Process(p) ‚àß Directive-ICE(o) ‚àß ‚àÉe,‚àÉi (Agent(e) ‚àß Intention(i) ‚àß bears(e,i) and concretizes(i,o) ‚àß realizes(p, i))" ;
             iof:source "Chris (wt Barry's preliminary approval)."@en ;
             iof:unresolved_issues "This needs to be reviewed." ;
             rdfs:label "achieves"@en .


###  http://www.industrialontologies.org/core#causes
iof:causes rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           iof:definition-FOL "causes(p, q) = def. process(p) & process(q) & ‚àÉx (participates-in(x, p) & participates-in(x, q) & ‚àÉd (has-disposition(x, d) & realizes(q, d) & triggers(p, q)"@en ;
           iof:source "Barry suggestion (extended relations ontology perhaps?)"@en ;
           rdfs:label "causes"@en .


###  http://www.industrialontologies.org/core#has_agent
iof:has_agent rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf bfoo:BFO_0000056 ;
              iof:definition-NL """A primitive relation between a process and an agent, which holds when the agent participates in the process and plays a causal role in bringing about the process. The causality here is what philosophers call ‚Äòagent causality‚Äô.
			   
x has_agent y ‚Üí agent(y) & process(x) & participates_in(x, y) & ‚àÉi ‚àÉp (intention_of(i, x) & realizes(p, i) & causes(p, y))"""@en ;
              iof:source "Barry suggestion (a refined veresion of what is in the agent ontology perhaps?)"@en ;
              iof:unresolved_issues "Needs to be reviewed. For starters, shouldn't it read \"x has-agent y ‚Üí process(x) & agent(y) & participates-in(y, x) & ‚àÉi ‚àÉp (intention-of(i, y) & realizes(p, i) & causes(p, x))?\" Have a look also at binary relation causes(p, q)."@en ;
              cco:example_of_usage "Example: Switching on of a machine has-agent machine operator"@en ;
              rdfs:label "has agent"@en .


###  http://www.industrialontologies.org/core#intention_of
iof:intention_of rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 iof:definition-FOL "intention_of(i, x) =def. agent(x) & intention(i) & bearer_of(x, i)"@en ;
                 iof:source "Barry suggestion (common core ontology perhaps?)"@en ,
                            "Neil & Barry."@en ;
                 iof:unresolved_issues "This needs to be reviewed." ;
                 rdfs:label "intention of"@en .


###  http://www.industrialontologies.org/core#is_available_to
iof:is_available_to rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain bfoo:BFO_0000004 ;
                    rdfs:range iof:Agent ;
                    iof:definition-NL """NL: independent continuant ‚Äòx‚Äô is available to agent ‚Äòy‚Äô means ‚Äòy‚Äô has some intention ‚Äòi‚Äô realized by some planned process ‚Äòp‚Äô whereby either ‚Äòx‚Äô has some disposition ‚Äòp‚Äô realizes or, more generally, ‚Äòx‚Äô merely participates in ‚Äòp‚Äô. 

FOL:  isAvailableTo (x, y) ‚Üí IndependentContinuant(x) ‚àß Agent(y) ‚àß ‚àÉp, ‚àÉi (PlannedProcess(p) ‚àß intentionOf(i, y) ‚àß realizes(p, i) ‚àß (‚àÉd (Disposition(d) ‚àß bears(x, d) ‚àß realizes(p, d)) ‚à® participates-in(x, p)))""" ;
                    rdfs:label "is available to" .


###  http://www.industrialontologies.org/core#specifies
iof:specifies rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf cco:prescribes ;
              rdfs:domain iof:Specification ;
              rdfs:range bfoo:BFO_0000001 ;
              iof:definition-FOL "specifies(x,y) ‚Üí Specification(x) ‚àß prescribes(x,y)" ;
              cco:elucidation "'x' specifies 'y' means:  'x' is a specification and  'x' prescribes 'y'" ;
              rdfs:label "specifies" .


###  http://www.industrialontologies.org/core#triggers
iof:triggers rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             iof:definition-NL """Triggers (as in process q triggers some other process q) is a primitive relation:

triggers(p, q) ü°™ ‚àÉt ‚àÉt' ‚àÉt\" (occupies-temporal-region(p, t) & occupies-temporal-region(q, t') & last-instant(p, t\") & first-instant(q, t\")"""@en ;
             iof:source "Barry suggestion (common core ontology perhaps?)"@en ;
             iof:unresolved_issues "This needs to be reviewed." ;
             cco:example_of_usage "An officer‚Äôs command triggers soldier‚Äôs action; dropping glass triggers glass breaking"@en ;
             rdfs:label "triggers"@en .


###  http://www.industrialontologies.org/core#uses_in
iof:uses_in rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            iof:definition-FOL "x isUsedIn y ‚Üí agent(x) & entity(y) & process(z) & participates-in(y, z)"@en ;
            iof:definition-NL "A primitive relation between an agent, an entity, and a process."@en ;
            iof:source "CCO"@en ;
            iof:unresolved_issues "This needs to be reviewed." ;
            cco:example_of_usage "A car is used in driving to work; a thermometer is used in measuring the temperature."@en ;
            rdfs:label "is used in"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/described_by
cco:described_by rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cco:is_subject_of ;
                 owl:inverseOf cco:describes ;
                 rdfs:range cco:DescriptiveInformationContentEntity ;
                 cco:definition "The inverse of describes, which relates an Entity to some Descriptive Information Content Entity."^^xsd:string ;
                 cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/InformationEntityOntology"^^xsd:string ;
                 rdfs:label "described by"^^xsd:string .


###  http://www.ontologyrepository.com/CommonCoreOntologies/describes
cco:describes rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf cco:is_about ;
              rdfs:domain cco:DescriptiveInformationContentEntity ;
              cco:definition "For all Types T1 and T2, if T1 describes T2 then there is some instance of T1, t1 that presents the characteristics by which some instance of T2, t2 can be recognized or visualized."^^xsd:string ;
              cco:example_of_usage "a newspaper article describes some current event"^^xsd:string ,
                                   "a visitor's log describes some facility visit"^^xsd:string ,
                                   "an accident report describes some accident"^^xsd:string ;
              cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/InformationEntityOntology"^^xsd:string ;
              rdfs:comment "It is possible that this relation should be a functional property, that is for all x, y, z if x describes y and x describes z then y = z. For example, if a financial report x describes the quarterly results of a company y and that same financial report describes the quarterly results of a company z, then it should be inferred that companies y and z are the same. We refrained from classifying the relation as a functional property on the concern that descriptions are multifaceted and so consequently it may be that the same report would contain descriptions of multiple entities."^^xsd:string ;
              rdfs:label "describes"^^xsd:string .


###  http://www.ontologyrepository.com/CommonCoreOntologies/designated_by
cco:designated_by rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf cco:is_subject_of ;
                  owl:inverseOf cco:designates ;
                  rdf:type owl:InverseFunctionalProperty ;
                  rdfs:range cco:DesignativeInformationContentEntity ;
                  cco:definition "The inverse of designates, which relates an Entity to some Designative Information Content Entity."^^xsd:string ;
                  cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/InformationEntityOntology"^^xsd:string ;
                  rdfs:label "designated by"^^xsd:string .


###  http://www.ontologyrepository.com/CommonCoreOntologies/designates
cco:designates rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf cco:is_about ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain cco:DesignativeInformationContentEntity ;
               cco:definition "For all types T1 and T2, if T1 designates T2, then there is some instance of T1, t1, by which some instance of T2, t2,  is called, or classified, distinguished from others."^^xsd:string ;
               cco:example_of_usage "a URL identifies the location of a Web Page on the internet"^^xsd:string ,
                                    "a person's name designates that person"^^xsd:string ,
                                    "a vehicle identification number designates some vehicle"^^xsd:string ;
               cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/InformationEntityOntology"^^xsd:string ;
               rdfs:label "designates"^^xsd:string .


