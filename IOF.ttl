                        iof-av:excerptedFrom "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:string ;
                        rdfs:label "is input of at some time"^^xsd:string ;
                        skos:definition "A relation between a Continuant and a Processual Entity in which the Continuant participates. The presence of the Continuant at the begining of the Processual Entity is a necessary condition for the start of the Processual Entity."^^xsd:string .


###  http://www.industrialontologies.org/core/isOutputOfAtSomeTime
iof:isOutputOfAtSomeTime rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf bfo:BFO_0000056 ;
                         rdfs:domain bfo:BFO_0000002 ;
                         rdfs:range bfo:BFO_0000015 ;
                         iof-av:excerptedFrom "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:string ;
                         rdfs:label "is output of at some time"^^xsd:string ;
                         skos:definition "A relation between a Continuant and a Processual Entity in which the Continuant participates. The presence of the Continuant at the end of the Processual Entity is a necessary condition for the completion of the Processual Entity."^^xsd:string .


###  http://www.industrialontologies.org/core/isSubjectOf
iof:isSubjectOf rdf:type owl:ObjectProperty ;
                rdfs:range iof:InformationContentEntity ;
                iof-av:excerptedFrom "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/InformationEntityOntology" ;
